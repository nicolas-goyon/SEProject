<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import com.SEApp.app.components.*?>
<VBox alignment="CENTER"
      prefHeight="239.0"
      prefWidth="243.0"
      spacing="20.0"
      stylesheets="@../Style/main.css"
      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.SEApp.app.controller.Login">
    <fx:include source="../components/Header.fxml"/>
    <Label fx:id="title" styleClass="h1" text="Login as member" />
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
<!--    <Header />-->
    <Label styleClass="h1" text="Login" />
    <Label text="Your login" />
    <TextField fx:id="email"  />
    <Label text="Your password" />
    <PasswordField fx:id="password" />
    <Button onAction="#onLogin" text="Login" />

    <HBox alignment="CENTER" spacing="10.0">
            <Label text="Enter your email and password to login" fx:id="error"/>
    </HBox>


</VBox>
